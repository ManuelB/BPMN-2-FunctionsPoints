<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:u="sap.ui.unified">
	<Dialog title="Select BPMN or XML file">
        <u:FileUploader
			tooltip="Select BPMN or XML file"
			fileType="bpmn,xml"
			width="400px"
			change="onFileSelected" multiple="true" />
			<endButton>
				<Button text="Cancel" press="onUploadCancel" />
			</endButton>
	</Dialog>
</core:FragmentDefinition>