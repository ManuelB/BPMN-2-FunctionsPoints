<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	controllerName="bpmn2fpa.controller.ProcessList">
	<Page title="Processes">
		<List items="{/files}" itemPress="onItemPress" mode="MultiSelect" id="list">
			<StandardListItem title="{name}" description="{lastModifiedDate}"
				type="Navigation" />
		</List>
		<footer>
			<Toolbar>
				<Button icon="sap-icon://delete" press="onDelete" />
				<ToolbarSpacer />
				<Button icon="sap-icon://add" press="onAdd" />
			</Toolbar>
		</footer>
	</Page>
</mvc:View>